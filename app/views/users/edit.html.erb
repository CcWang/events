<a href="/events">Events Dashboard</a>
<div>
  <h1>Edit Your Profile</h1>
  <form action="/users/<%=@user.id%>" method="post">
    <input type="hidden" name="authenticity_token" value="<%=form_authenticity_token%>"> 
    <input type="hidden" name="_method" value="patch"> 
    <p>First Name:
    <input type="text" name="first_name" value="<%=@user.first_name%>"><br></p>
    <p>Last Name:
    <input type="text" name="last_name" value="<%=@user.last_name%>"><br></p>
    <p>Email:
    <input type="email" name="email" value="<%=@user.email%>"><br></p>
    <p>Location:
    <input type="text" name="location"value="<%=@user.location%>">
    State:
     <input type="text" name="state" value="<%=@user.state%>"><br></p>
    <input type="submit" value="Update Records"><br>
  </form>
</div>